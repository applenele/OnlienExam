<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
       
 <hibernate-mapping package="org.nele.domain"> 
       <class name="Major" table="ex_major">
         <id name="majorId" column="majorId" type="java.lang.Integer">
           <generator class="native"></generator>
         </id>
         
         <property name="xyId" column="xyId" type="java.lang.Integer"></property>
         <property name="majorName" column="majorName" type="java.lang.String"></property>
       </class>
  </hibernate-mapping>
