<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
        
<hibernate-mapping package="org.nele.domain">
     <class name="Class" table="ex_class">
         <id name="classId" column="classId" type="java.lang.Integer">
           <generator class="native"></generator>
         </id>
         
         <property name="className" column="className" type="java.lang.String"></property>
         <property name="classGrade" column="classGrade" type="java.lang.String"></property>
         <property name="majorId" column="majorId" type="java.lang.Integer"></property>
     </class>
   </hibernate-mapping>
